Pattern 6
    *
   ***
  *****
 *******
*********

//code
#include <bits/stdc++.h>
using namespace std;
int main(){
    int i,j,n;
    cout<<"Enter the value for n: ";
    cin>>n;
    for(i=1;i<=5;i++){
        for(j=1;j<=9;j++){
            if(j>=6-i && j<=4+i){
                cout<<"*";
            }
            else{
                cout<<" ";
            }
        }
        cout<<endl;
    }
}
